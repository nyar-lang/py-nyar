@external(rust)
variant Json {
    #
    Null,
    #
    Boolean(bool),
    #
    Number(Number),
    #
    String(String),
    #
    Array(Vector[Json]),
    #
    Object(OrderedMap[String, Json]),
}

@external(rust)
variant Number {
    # Always larger than zero.
    Postive(u64),
    # Always less than zero.
    Negtive(i64),
    # Always finite.
    Float(f64),
}


